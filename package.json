{
  "name": "@kunukn/tween",
  "version": "0.0.1",
  "description": "tween",
  "main": "dist/tween.js",
  "scripts": {
    "build": "npx babel src/tween.js --out-file dist/tween.js",
    "deploy":
      "npx babel src/tween.js --presets=babili --out-file dist/tween.min.js "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunukn/tween.git"
  },
  "files": ["dist"],
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "env": {
      "production": {
        "presets": ["babili"]
      }
    }
  },
  "author": "kunukn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kunukn/tween/issues"
  },
  "homepage": "https://github.com/kunukn/tween#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-es2015": "^6.24.1"
  }
}
