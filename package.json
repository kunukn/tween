{
  "name": "@kunukn/tween",
  "version": "0.0.8",
  "description": "tween",
  "main": "dist/tween.umd.js",
  "scripts": {
    "start": "node src/example.js",
    "umd": "npx babel src/tween.js --no-babelrc --presets=es2015 --plugins transform-es2015-modules-umd --out-file dist/tween.umd.js",
    "umd-min": "npx babel src/tween.js --no-babelrc --presets=es2015,babili --plugins transform-es2015-modules-umd --out-file dist/tween.umd.min.js",
    "deploy": "npm run umd & npm run umd-min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunukn/tween.git"
  },
  "files": [
    "dist"
  ],
  "author": "kunukn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kunukn/tween/issues"
  },
  "homepage": "https://github.com/kunukn/tween#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-es2015": "^6.24.1"
  }
}
